{
    "name": "Facebook - UnFollowAll",
    "description": "You can un follow all your following people with one click !!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon48.png"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}